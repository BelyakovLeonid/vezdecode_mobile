<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="title" format="string" />
    <attr name="hint" format="string" />

    <declare-styleable name="DonateTypeView">
        <attr name="title" />
        <attr name="subTitle" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="SetupFieldView">
        <attr name="title" />
        <attr name="hint" />
        <attr name="inputType" format="enum">
            <enum name="text" value="0"/>
            <enum name="numbers" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SetupExpandableView">
        <attr name="title" />
        <attr name="hint" />
    </declare-styleable>
</resources>